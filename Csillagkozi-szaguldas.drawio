        <mxCell id="uCUTDOpJi0z06TZupSKV-41" value="Ürhajók bemutatása&amp;nbsp;" style="text;html=1;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;" vertex="1" parent="1">
